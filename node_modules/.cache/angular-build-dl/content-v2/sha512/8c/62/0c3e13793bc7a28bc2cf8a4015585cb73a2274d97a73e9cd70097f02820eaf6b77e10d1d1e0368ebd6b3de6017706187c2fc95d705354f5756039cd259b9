  }\r\n        ]\r\n      }\r\n    });\r\n  }\r\n  doRefresh(event) {\r\n    console.log('Begin async operation');\r\n\r\n    setTimeout(() => {\r\n      console.log('Async operation has ended');\r\n      event.target.complete();\r\n    }, 2000);\r\n  }\r\n  async presentLoading() {\r\n    const loading = await this.loadingCtrl.create({\r\n      message: 'Please wait...'\r\n    });\r\n    await loading.present();\r\n  }\r\n}\r\n"],"sourceRoot":"webpack:///"}